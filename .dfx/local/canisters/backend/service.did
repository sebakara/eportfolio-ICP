type Item = 
 record {
   description: text;
   image: blob;
   title: text;
 };
type InstitutionOverview = 
 record {
   id: InstitutionId;
   item: Item;
 };
type InstitutionId = nat;
type InstitutionDetails = record {item: Item;};
service : {
  getInstitutionDetails: (InstitutionId) -> (InstitutionDetails) query;
  getOverviewList: () -> (vec InstitutionOverview) query;
  newInstitution: (Item) -> ();
}
